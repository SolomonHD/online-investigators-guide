<aside class="column is-3 is-narrow-mobile is-hidden-mobile">
  <div class="menu sub-menu ">
    <% if params[:user] and params[:view] %>
      <% if not current_user.id.to_s == params[:user] %>
        <%= render :partial => 'unfilteredSideMenu' %>
      <% else %>
        <%= render :partial => 'filteredSideMenu' %>
      <% end %>
    <% else %>
      <%= render :partial => 'unfilteredSideMenu' %>
    <% end %>
  </div>
  <%= render :partial => 'feedbackSurvey' %>
</aside>
